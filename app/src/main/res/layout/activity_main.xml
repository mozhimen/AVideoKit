<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".RTSPMainActivity">

        <cn.nodemedia.NodePlayerView
            android:id="@+id/main_rtsp_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

        <com.mozhimen.xmlk.textk.TextKClock
            style="@style/StyleK_Txt.Font_Sans_Serif.Gravity_Center.Style_Norm.Color_Gray_000.Size_014.Width_Wrap.Height_Wrap"
            android:layout_margin="@dimen/dp_010"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:textkClock_timeFormat="HHmmss" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>